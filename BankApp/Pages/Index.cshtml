@page "{number?}"

@model BankApp.Pages.IndexModel
@{
    ViewData["Title"] = "Index";
}

<h2>Users</h2>
<hr />

@foreach (var user in Model.Users)
{
    <p> @user.Givenname @user.Maidenname </p>
}

<Pagination page-count="@Model.TotalPages" page-number="@Model.PageNumber" page-range="5" page-first="اول" page-last="آخر"></Pagination>